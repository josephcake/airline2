<style>
@import url('https://fonts.googleapis.com/css?family=Quicksand');
</style>
<style>
@import url('https://fonts.googleapis.com/css?family=VT323');
</style>
<header>
  <div class="navbar">
    <img class="logo_pic" src="https://image.flaticon.com/icons/svg/929/929900.svg" alt="">
    <div class="dropdown">
      <button class="dropbtn">Menu
        <i class="fa fa-caret-down"></i>
      </button>
      <div class="dropdown-content">
        <a href="/home">Home</a>
        <a href="/customers/<%= current_user.id %>/edit" >My profile</a>
        <a href="/customer_tickets/:id">My flights</a>
        <a href="/logout">Logout</a>
      </div>
    </div>
  </div>
</header>

<h1>All Airports' name</h1>

<table border="1">
  <tr>
    <th>Airport</th>
    <th>City</th>
  </tr>
  <% @airports.each do |row| %>
  <% row.id %>
  <tr>
    <td><%= row.airport_name%></td>
    <td><%= row.city %></td>
  </tr>
  <% end %>
</table>
